{
  "name": "@lens-protocol/client",
  "version": "0.1.0",
  "description": "Low level Lens API client",
  "repository": {
    "directory": "packages/client",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "private": true,
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "pnpm clean && tsup",
    "clean": "rimraf ./dist",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen-api.yml"
  },
  "license": "MIT",
  "dependencies": {
    "@lens-protocol/shared-kernel": "workspace:*",
    "@lens-protocol/storage": "workspace:*",
    "graphql": "^15.5.1",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "tslib": "^2.4.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.7",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.11",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "ethers": "^5.7.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "prettier": "@lens-protocol/prettier-config"
}

{
  "name": "@lens-protocol/wagmi",
  "version": "0.0.1",
  "description": "wagmi bindings for @lens-protocol/react",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "clean": "rimraf ./dist",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit"
  },
  "license": "ISC",
  "devDependencies": {
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@lens-protocol/react": "workspace:*",
    "ethers": "^5.7.2",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "wagmi": "0.9.2"
  },
  "peerDependencies": {
    "@lens-protocol/react": "workspace:*",
    "ethers": "^5.7.2",
    "wagmi": "0.9.2"
  },
  "prettier": "@lens-protocol/prettier-config",
  "dependencies": {
    "@lens-protocol/shared-kernel": "workspace:^0.0.1"
  }
}

{
  "name": "@lens-protocol/graphql",
  "version": "0.1.0-alpha.0",
  "description": "GraphQL bindings for the Lens API",
  "repository": {
    "directory": "packages/graphql",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.ts",
      "import": "./dist/test-utils.js",
      "require": "./dist/test-utils.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "import": [
        "./dist/index.d.ts"
      ],
      "require": [
        "./dist/index.d.cts"
      ],
      "test-utils": [
        "./dist/test-utils.d.ts"
      ],
      "schema": [
        "./dist/schema.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "check": "gql.tada check",
    "doctor": "gql.tada doctor",
    "gql:generate:cache": "gql.tada generate turbo",
    "gql:download:local": "./scripts/fetch-schema.ts http://localhost:3000/graphql ./src/schema.json && gql-tada generate schema 'http://localhost:3000/graphql' --output './schema.graphql'",
    "gql:download:staging": "./scripts/fetch-schema.ts https://api.staging.lens.dev/graphql ./src/schema.json && gql-tada generate schema 'https://api.staging.lens.dev/graphql' --output './schema.graphql'",
    "gql:generate": "gql-tada generate output",
    "gql:turbo": "gql-tada turbo",
    "prebuild": "pnpm run gql:turbo"
  },
  "dependencies": {
    "@lens-protocol/types": "workspace:*",
    "gql.tada": "^1.8.10",
    "graphql": "^16.9.0",
    "type-fest": "^4.26.1"
  },
  "devDependencies": {
    "@urql/introspection": "^1.2.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.3",
    "typescript": "^5.6.3"
  },
  "license": "MIT"
}

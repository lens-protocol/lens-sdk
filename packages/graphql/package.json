{
  "name": "@lens-protocol/graphql",
  "version": "0.1.0",
  "description": "GraphQL bindings for the Lens API",
  "repository": {
    "directory": "packages/graphql",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./test-utils": {
      "import": "./dist/test-utils.js",
      "require": "./dist/test-utils.cjs",
      "types": "./dist/test-utils.d.cts"
    },
    "./schema.json": {
      "import": "./src/schema.json",
      "require": "./src/schema.json"
    }
  },
  "typesVersions": {
    "*": {
      "import": ["./dist/index.d.ts"],
      "require": ["./dist/index.d.cts"],
      "test-utils": ["./dist/test-utils.d.ts"]
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "check": "gql.tada check",
    "doctor": "gql.tada doctor",
    "introspect:local": "BASE_URL=http://localhost:3000/graphql ./scripts/introspection-curl.sh",
    "introspect:staging": "BASE_URL=https://api.staging.lens.dev/graphql ./scripts/introspection-curl.sh",
    "gql:generate:cache": "gql.tada generate turbo",
    "schema:local": "gql-tada generate schema 'http://localhost:3000/graphql' --output './schema.graphql'",
    "schema:staging": "gql-tada generate schema 'https://api.staging.lens.dev/graphql' --output './schema.graphql'",
    "gql:download:local": "pnpm run schema:local && pnpm run introspect:local",
    "gql:download:staging": "pnpm run schema:staging && pnpm run introspect:staging",
    "gql:generate": "gql-tada generate output",
    "gql:turbo": "gql-tada turbo",
    "prebuild": "pnpm run gql:turbo"
  },
  "dependencies": {
    "@lens-protocol/types": "workspace:*",
    "gql.tada": "^1.8.10",
    "graphql": "^16.9.0",
    "type-fest": "^4.26.1"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "license": "MIT"
}

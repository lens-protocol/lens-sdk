{
  "name": "@lens-protocol/shared-kernel",
  "version": "0.1.1",
  "description": "Shared kernel",
  "repository": {
    "directory": "packages/shared-kernel",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./mocks": {
      "import": {
        "types": "./dist/esm/mocks.d.ts",
        "default": "./dist/esm/mocks.js"
      },
      "require": {
        "types": "./dist/cjs/mocks.d.ts",
        "default": "./dist/cjs/mocks.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "mocks"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.build-cjs.json",
    "build:esm": "tsc -p ./tsconfig.build.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "clean": "rimraf ./dist",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "tsc": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.191",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.28.0",
    "ethers": "^5.7.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "ethers": "^5.7.2"
  },
  "prettier": "@lens-protocol/prettier-config"
}

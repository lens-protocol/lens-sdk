{
  "name": "@lens-protocol/sdk-gated",
  "version": "1.2.0",
  "private": true,
  "description": "SDK for encrypting and decrypting gated content on Lens Protocol",
  "author": "zannis@aave.com",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "pnpm rollup:prod",
    "clean": "rimraf ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "pnpm run prettier && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "rollup": "pnpm run clean && rollup -c rollup.config.mjs",
    "rollup:prod": "pnpm run clean && NODE_ENV=production rollup -c rollup.config.mjs",
    "codegen": "graphql-codegen",
    "test:browser": "jest -c jest.config.browser.ts",
    "test:node": "jest -c jest.config.node.ts",
    "test": "echo \"Tests are not stable, package deprecated.\" && exit 0",
    "tsc": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/strings": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@metamask/detect-provider": "^2.0.0",
    "pako": "^2.1.0",
    "siwe": "^1.1.6",
    "uint8arrays": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@graphql-codegen/cli": "^3.1.0",
    "@graphql-codegen/fragment-matcher": "^4.0.1",
    "@graphql-codegen/typed-document-node": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@peculiar/webcrypto": "^1.4.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "29.2.3",
    "@types/node": "^18.15.11",
    "@types/pako": "^2.0.0",
    "blob-polyfill": "^7.0.20220408",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup": "^3.15.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}

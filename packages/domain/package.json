{
  "name": "@lens-protocol/domain",
  "version": "0.0.1",
  "description": "Critical Business Rules and Application-specific Business Rules",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "clean": "rimraf ./dist",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "tsc": "tsc --noEmit"
  },
  "license": "ISC",
  "dependencies": {
    "@lens-protocol/shared-kernel": "workspace:*"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.3.1",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@types/jest": "^29.2.3",
    "@types/jest-when": "^3.5.2",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-when": "^3.5.2",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "wait-for-expect": "^3.0.2"
  },
  "peerDependencies": {
    "tslib": "^2.4.1"
  },
  "prettier": "@lens-protocol/prettier-config"
}

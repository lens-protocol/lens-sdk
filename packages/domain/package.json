{
  "name": "@lens-protocol/domain",
  "version": "0.1.1",
  "description": "Critical Business Rules and Application-specific Business Rules",
  "repository": {
    "directory": "packages/domain",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./entities": {
      "import": {
        "types": "./dist/esm/entities/index.d.ts",
        "default": "./dist/esm/entities/index.js"
      },
      "require": {
        "types": "./dist/cjs/entities/index.d.ts",
        "default": "./dist/cjs/entities/index.js"
      }
    },
    "./use-cases": {
      "import": {
        "types": "./dist/esm/use-cases/index.d.ts",
        "default": "./dist/esm/use-cases/index.js"
      },
      "require": {
        "types": "./dist/cjs/use-cases/index.d.ts",
        "default": "./dist/cjs/use-cases/index.js"
      }
    },
    "./use-cases/*": {
      "import": {
        "types": "./dist/esm/use-cases/*/index.d.ts",
        "default": "./dist/esm/use-cases/*/index.js"
      },
      "require": {
        "types": "./dist/cjs/use-cases/*/index.d.ts",
        "default": "./dist/cjs/use-cases/*/index.js"
      }
    },
    "./mocks": {
      "import": {
        "types": "./dist/esm/mocks.d.ts",
        "default": "./dist/esm/mocks.js"
      },
      "require": {
        "types": "./dist/cjs/mocks.d.ts",
        "default": "./dist/cjs/mocks.js"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "entities",
    "use-cases",
    "mocks"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.build-cjs.json",
    "build:esm": "tsc -p tsconfig.build.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "clean": "rimraf ./dist",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "tsc": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@lens-protocol/shared-kernel": "workspace:*",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.3.1",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@types/jest": "^29.2.3",
    "@types/jest-when": "^3.5.2",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-when": "^3.5.2",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "wait-for-expect": "^3.0.2"
  },
  "prettier": "@lens-protocol/prettier-config"
}

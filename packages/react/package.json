{
  "name": "@lens-protocol/react",
  "version": "0.3.0",
  "description": "Interacting with the Lens Protocol API using React.",
  "main": "dist/lens-protocol-react.cjs.js",
  "module": "dist/lens-protocol-react.esm.js",
  "exports": {
    ".": {
      "module": "./dist/lens-protocol-react.esm.js",
      "default": "./dist/lens-protocol-react.cjs.js"
    },
    "./web": {
      "module": "./web/dist/lens-protocol-react-web.esm.js",
      "default": "./web/dist/lens-protocol-react-web.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "directory": "packages/react",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "web"
  ],
  "scripts": {
    "build": "preconstruct build",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@lens-protocol/api-bindings": "workspace:*",
    "@lens-protocol/blockchain-bindings": "workspace:*",
    "@lens-protocol/domain": "workspace:*",
    "@lens-protocol/shared-kernel": "workspace:*",
    "@lens-protocol/storage": "workspace:*",
    "graphql": "^15.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@ensdomains/ens": "0.4.4",
    "@ensdomains/resolver": "0.2.4",
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/bytes": "5.7.0",
    "@faker-js/faker": "^7.6.0",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/jest-when": "^3.5.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ethereum-waffle": "^4.0.9",
    "ethers": "^5.7.2",
    "ganache": "7.7.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-when": "^3.5.2",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolver:@0.2.4": "link:@ensdomains/resolver:@0.2.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "ethers": "^5.7.2",
    "react": "^18.2.0"
  },
  "prettier": "@lens-protocol/prettier-config",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "preconstruct": {
    "entrypoints": [
      "index.tsx",
      "web/index.ts"
    ],
    "exports": true
  }
}

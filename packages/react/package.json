{
  "name": "@lens-protocol/react",
  "version": "0.0.1",
  "description": "Interacting with the Lens Protocol API using React.",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/esm/web.d.ts",
        "default": "./dist/esm/web.js"
      },
      "require": {
        "types": "./dist/cjs/web.d.ts",
        "default": "./dist/cjs/web.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "web"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "clean": "rimraf ./dist",
    "eslint:fix": "pnpm run eslint --fix",
    "eslint": "eslint src",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@lens-protocol/api": "workspace:*",
    "@lens-protocol/domain": "workspace:^0.0.1",
    "@lens-protocol/shared-kernel": "workspace:*",
    "@lens-protocol/storage": "workspace:*",
    "graphql": "15.5.1",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/prettier-config": "workspace:*",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/jest-when": "^3.5.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-when": "^3.5.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0"
  },
  "prettier": "@lens-protocol/prettier-config"
}

{
  "name": "@lens-protocol/react",
  "version": "3.0.0-alpha.0",
  "description": "The official React bindings for the Lens Protocol",
  "repository": {
    "directory": "packages/react",
    "type": "git",
    "url": "git://github.com/lens-protocol/lens-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./ethers": {
      "import": "./dist/ethers.js",
      "require": "./dist/ethers.cjs",
      "types": "./dist/ethers.d.cts"
    },
    "./viem": {
      "import": "./dist/viem.js",
      "require": "./dist/viem.cjs",
      "types": "./dist/viem.d.cts"
    }
  },
  "typesVersions": {
    "*": {
      "ethers": [
        "./dist/ethers.d.ts"
      ],
      "viem": [
        "./dist/viem.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lens-protocol/client": "workspace:*",
    "@lens-protocol/env": "workspace:*",
    "@lens-protocol/graphql": "workspace:*",
    "@lens-protocol/types": "workspace:*",
    "urql": "^4.2.1"
  },
  "devDependencies": {
    "@lens-protocol/metadata": "^2.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "ethers": "^6.13.4",
    "happy-dom": "^15.7.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "viem": "^2.22.4",
    "zksync-ethers": "^6.15.3"
  },
  "peerDependencies": {
    "@types/react": "^18.3.12 || ^19.0.0",
    "ethers": "^6.13.4",
    "react": "^18.3.1 || ^19.0.0",
    "viem": "^2.21.33",
    "zksync-ethers": "^6.15.3"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "ethers": {
      "optional": true
    },
    "viem": {
      "optional": true
    },
    "zksync-ethers": {
      "optional": true
    }
  },
  "license": "MIT"
}

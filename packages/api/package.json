{
  "name": "@lens-protocol/api",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "clean": "rimraf ./dist",
    "test": "jest --passWithNoTests",
    "eslint": "eslint",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "tsc": "tsc --noEmit",
    "gql-codegen": "graphql-codegen --config codegen-api.yml"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@graphql-codegen/cli": "^1.21.7",
    "@graphql-codegen/fragment-matcher": "^3.1.0",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.10",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-react-apollo": "^2.3.1",
    "@graphql-codegen/typescript": "^1.23.0",
    "@lens-protocol/eslint-config": "workspace:*",
    "@lens-protocol/shared-kernel": "workspace:*",
    "@types/jest": "^29.2.3",
    "babel-plugin-graphql-tag": "^3.3.0",
    "graphql-tag": "^2.12.6",
    "graphql": "15.5.1",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "eslint": "^8.28.0"
  }
}
